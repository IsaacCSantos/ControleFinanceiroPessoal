program ControleFinanceiro;

uses
  Vcl.Forms,
  UnitPrincipal in 'UnitPrincipal.pas' {Form1},
  UnitFormCategorias in 'View\UnitFormCategorias.pas' {FormCategorias},
  UnitFormLogin in 'View\UnitFormLogin.pas' {FormLogin},
  UnitFormPrincipal in 'View\UnitFormPrincipal.pas' {FormPrincipal},
  UnitFormRegister in 'View\UnitFormRegister.pas' {FormRegister};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFormCategorias, FormCategorias);
  Application.CreateForm(TFormLogin, FormLogin);
  Application.CreateForm(TFormPrincipal, FormPrincipal);
  Application.CreateForm(TFormRegister, FormRegister);
  Application.Run;
end.
